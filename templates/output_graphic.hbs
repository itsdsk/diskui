<svg width="{{window.width}}" height="{{window.height}}">
    {{#each outputs}}
    <g data-output-id="{{index}}">
        {{#each leds}}
        {{#unless @first}}
        x2="{{x}}" y2="{{y}}" />
        {{/unless}}
        {{#unless @last}}
        <line class="line" data-from-circle-id="{{index}}" x1="{{x}}" y1="{{y}}"
        {{/unless}}
        {{/each}}
        {{#each leds}}
        <line x1="{{x}}" y1="{{y}}" x2="{{x}}" y2="{{y}}" data-radius-id="{{r}}" data-circle-id="{{index}}" class="circle"/>
        {{/each}}
    </g>
    {{/each}}
</svg>